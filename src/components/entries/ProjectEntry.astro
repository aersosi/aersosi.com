---
import EntryWrapper from "./EntryWrapper.astro";

const { url, viewerTitle, viewerDescription, projectType } = {
	url: "/projects/" + Astro.props.project.slug,
	viewerTitle: Astro.props.project.data.viewerTitle,
	viewerDescription: Astro.props.project.data.viewerDescription,
	projectType: Astro.props.project.data.tags[0],
};
---

<EntryWrapper url={url} viewerTitle={viewerTitle} viewerDescription={viewerDescription}>
	{projectType}
</EntryWrapper>
