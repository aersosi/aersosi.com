---
import EntryWrapper from "./EntryWrapper.astro";

const { url, title, description, projectType } = {
	url: "/projects/" + Astro.props.project.slug,
	title: Astro.props.project.data.viewerTitle,
	description: Astro.props.project.data.viewerDescription,
	projectType: Astro.props.project.data.tags[0],
};
---

<EntryWrapper url={url} title={title} description={description}>
	{projectType}
</EntryWrapper>
