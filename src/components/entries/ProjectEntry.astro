---
import EntryWrapper from "./EntryWrapper.astro";

const { url, viewerTitle, pageDescription, projectType } = {
	url: "/projects/" + Astro.props.project.slug,
	viewerTitle: Astro.props.project.data.viewerTitle,
	pageDescription: Astro.props.project.data.pageDescription,
	projectType: Astro.props.project.data.tags[0],
};
---

<EntryWrapper url={url} viewerTitle={viewerTitle} pageDescription={pageDescription}>
	{projectType}
</EntryWrapper>
